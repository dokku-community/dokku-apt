#!/usr/bin/env bash
export PLUGIN_UNIMPLEMENTED_SUBCOMMANDS=()
export PLUGIN_COMMAND_PREFIX="apt"
export PLUGIN_CONFIG_ROOT="$DOKKU_LIB_ROOT/config/$PLUGIN_COMMAND_PREFIX"
export PLUGIN_DATA_ROOT="$DOKKU_LIB_ROOT/data/$PLUGIN_COMMAND_PREFIX"
export PLUGIN_BASE_PATH="$PLUGIN_PATH"
if [[ -n $DOKKU_API_VERSION ]]; then
  export PLUGIN_BASE_PATH="$PLUGIN_ENABLED_PATH"
fi
